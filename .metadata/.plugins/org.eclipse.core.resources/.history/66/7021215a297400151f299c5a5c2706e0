package ev3ObjectRecognition;

import lejos.hardware.*;
import lejos.hardware.ev3.LocalEV3;
import lejos.hardware.motor.EV3LargeRegulatedMotor;
import lejos.hardware.port.Port;
import lejos.hardware.sensor.*;
import lejos.robotics.SampleProvider;

public class Scan {
	
	private static EV3ColorSensor lightSensor;
	private static EV3UltrasonicSensor usSensor;
	private Odometer odo;
	private Navigation nav;
	private ObjectRecognition OR;
	
	public Scan (EV3UltrasonicSensor usSensor, EV3ColorSensor lightSensor, Odometer odo, Navigation nav, ObjectRecognition OR){
		this.usSensor = usSensor;
		this.lightSensor = lightSensor;
		this.odo = odo;
		this.nav = nav;
		this.OR = OR;
	}

}
